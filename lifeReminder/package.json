{
  "name": "lifereminder",
  "version": "1.0.0",
  "scripts": {
    "dev": "cd frontend && npx expo start --android ",
    "start": "cd backend && npm start",
    "android": "cd frontend && expo start --android",
    "ios": "cd frontend && expo start --ios",
    "web": "cd frontend && expo start --web",
    "all": "concurrently \"npm run start\" \"npm run dev\""
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "dependencies": {
    "@react-native-community/checkbox": "^0.5.20"
  }
}